{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "strict": 0,
    "import/no-unresolved": [2, { "ignore": ["shared\/"] } ],
    "react/jsx-uses-vars": 1
  },
  "globals": {
    "DatePickerModal": true,
    "preact": true,
    "React": true,
    "ReactDOM": true
  }
}
